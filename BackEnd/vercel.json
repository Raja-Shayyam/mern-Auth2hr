
{
  "version": 2,
  "builds": [
    {
      "src": "Server.js",
      "use": "@vercel/node",
      "config": {
        "distDir": "frontend/build"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "Server.js"
    }
  ]
}

